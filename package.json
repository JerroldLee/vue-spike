{
  "name": "vue-spike",
  "version": "1.0.0",
  "description": "Server-side rendering example for Vue.js components",
  "main": "webpack.config.js",
  "scripts": {
    "dev:watch": "webpack --watch --config webpack.config.js",
    "dev:server": "webpack-dev-server --inline --hot --config webpack.config.js",
    "start": "node src/server/run.js",
    "update-vue": "npm update vue && cp -r node_modules/vue/dist vue"
  },
